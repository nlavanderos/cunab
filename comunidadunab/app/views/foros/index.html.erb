<p id="notice"><%= notice %></p>

<h1>Foros</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Anuncio</th>
      <th>Foro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foros.each do |foro| %>
      <tr>
        <td><%= foro.id %></td>
        <td><%= foro.anuncio %></td>
        <td><%= foro.foro %></td>
        <td><%= link_to 'Show', foro %></td>
        <td><%= link_to 'Edit', edit_foro_path(foro) %></td>
        <td><%= link_to 'Destroy', foro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foro', new_foro_path %>
